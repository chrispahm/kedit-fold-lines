{
  "name": "fold-lines",
  "displayName": "fold-lines",
  "description": "Filter lines (grep), without modifying",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fold.filter",
        "title": "Filter lines by regex"
      },
      {
        "command": "fold.more",
        "title": "Show more lines by regex"
      },
      {
        "command": "fold.less",
        "title": "Show less lines by regex"
      },
      {
        "command": "fold.clear",
        "title": "Clear all folds"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  }
}
